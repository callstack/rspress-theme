{
  "name": "@callstack/rspress-theme",
  "version": "0.2.0",
  "description": "Callstack theme for Rspress docs",
  "author": "Jakub Roma≈Ñczyk <jakub.romanczyk@callstack.com>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./plugin": {
      "types": "./dist/plugin/index.d.ts",
      "import": "./dist/plugin/index.js",
      "require": "./dist/plugin/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": ["dist", "plugin", "README.md"],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://github.com/callstack/rspress-theme/tree/main/packages/theme#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/callstack/rspress-theme.git",
    "directory": "packages/theme"
  },
  "keywords": [
    "rspress",
    "theme",
    "callstack",
    "documentation",
    "react",
    "typescript"
  ],
  "packageManager": "pnpm@9.12.2",
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "rspress": "^2.0.0-beta.21"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.52.8",
    "@rsbuild/plugin-image-compress": "^1.2.0",
    "@rsbuild/plugin-react": "^1.3.4",
    "@rsbuild/plugin-sass": "^1.3.3",
    "@rsbuild/plugin-svgr": "^1.2.1",
    "@rslib/core": "^0.10.5",
    "@types/node": "^22",
    "@types/react": "^19",
    "react": "^19",
    "rspress": "^2.0.0-beta.21",
    "typescript": "^5.8.3"
  }
}
