{
  "name": "@callstack/rspress-theme",
  "version": "0.0.3",
  "description": "Callstack theme for Rspress docs",
  "author": "Jakub Roma≈Ñczyk <jakub.romanczyk@callstack.com>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./plugin": {
      "types": "./dist/plugin/index.d.ts",
      "import": "./dist/plugin/index.js",
      "require": "./dist/plugin/index.cjs"
    }
  },
  "files": ["dist", "plugin", "static", "README.md"],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://github.com/callstack/rspress-theme/tree/main/packages/theme#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/callstack/rspress-theme.git",
    "directory": "packages/theme"
  },
  "keywords": [
    "rspress",
    "theme",
    "callstack",
    "documentation",
    "react",
    "typescript"
  ],
  "packageManager": "pnpm@9.12.2",
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.48.0",
    "@rsbuild/plugin-react": "^1.1.0",
    "@rsbuild/plugin-sass": "^1.1.2",
    "@rslib/core": "^0.1.3",
    "@rspress/shared": "^1.34.1",
    "@types/node": "^22.7.7",
    "@types/react": "^18.3.12",
    "react": "^18.3.1",
    "typescript": "^5.6.3"
  }
}
